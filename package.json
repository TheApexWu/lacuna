{
  "name": "lacuna-scaffold",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "dev:python": "cd python && source .venv/bin/activate && uvicorn app.main:app --reload --port 8000",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:python\"",
    "python:setup": "cd python && uv pip install -r requirements.txt",
    "python:export": "node scripts/export_concepts.js",
    "python:extract": "cd python && source .venv/bin/activate && python scripts/extract_embeddings.py",
    "python:fit": "cd python && source .venv/bin/activate && python scripts/fit_umap.py"
  },
  "dependencies": {
    "@mistralai/mistralai": "^1.14.1",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@react-three/postprocessing": "^3.0.4",
    "@types/three": "^0.183.1",
    "gsap": "^3.14.2",
    "leva": "^0.10.1",
    "next": "16.1.6",
    "postprocessing": "^6.38.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "three": "^0.183.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
